{% extends 'site/eventlist/page.mainTab.html.twig' %}

{% block bodyClass %}calendarWidth{% endblock %}

{% block pageTitle %}Events - {% endblock %} 


{% block htmlHeader %}
	<link rel="stylesheet" type="text/css" href="/theme/default/css/calendar.css?v={{ config.assetsVersion }}">
	{% if not currentSite.isWebRobotsAllowed %}<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">{% endif %}
{% endblock %}

{% block breadcrumbs %}
	{%  include 'site/breadcrumbs.html.twig' with { 'breadcrumbs': [  { 'url':'/event', 'label':'events' } ]  } %}
{% endblock %}

{% block switcherMonthly %}current{% endblock %}

{% block eventsContent %}


	{%  include '/site/common/calendar.monthly.html.twig' with {'addURL':'/event/new', 'calURL':'/event/calendar' } %}

{% endblock %}


{% block scriptsForState %}
	<script type="text/javascript">var exportData = {}</script>
{% endblock %}

{% block scripts %}
	<script type="text/javascript">
		var showCurrentUserOptions = {% if currentUser is not null and showCurrentUserOptions is defined and showCurrentUserOptions %}true{% else %}false{% endif %};
	</script>
	<script src="/theme/default/js/eventFilterParams.js?v={{ config.assetsVersion }}" type="text/javascript"></script>
	<script src="/theme/default/js/calendar.site.js?v={{ config.assetsVersion }}" type="text/javascript"></script>
{% endblock %}
